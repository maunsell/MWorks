    <isa>Stimulus</isa>
    
    <description>
      A ellipse stimulus
    </description>
    
    <!-- Payload -->
    <code>
      <stimulus type="circle" tag="New Ellipse"
                color="1,1,1"
		x_size=""
		y_size=""
		x_position=""
		y_position=""
		rotation=""
		contrast=""/>
    </code>
  </MWElement>
</MWElements>
